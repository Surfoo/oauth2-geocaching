{
    "name": "surfoo/oauth2-geocaching",
    "description": "Geocaching OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Surfoo",
            "email": "surfooo@gmail.com",
            "homepage": "https://github.com/Surfoo"
        }
    ],
    "keywords": [
        "oauth2",
        "client",
        "authorization",
        "geocaching",
        "groundspeak",
        "pkce"
    ],
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/php-code-coverage": "^5.0|^6.0",
        "phpunit/phpunit": "^6.0|^7.0",
        "jakub-onderka/php-parallel-lint": "~1.0",
        "mockery/mockery": "~1.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs src --standard=psr2 -sp",
        "phpunit": "./vendor/bin/phpunit --verbose "
    }
}
